<%- include("header"); -%>

<div class="box" id="heading">
    <h1> <%=listTitle%> </h1>
</div>

<div class="box">
    <%if(items.length!=0){%>
        <% for(var i=0;i<items.length;i++){ %>
            <div class="item">
            <input type="checkbox">
            <p> <%= items[i].name %> </p>             
            </div>
        <% } %>  
    <%}else{%>      
        <div class="item">
            <p> There is no item in this date </p>
        </div>
    <%}%>    
</div>

<%- include("footer")-%>